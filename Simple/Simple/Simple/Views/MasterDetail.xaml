<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage 
	xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:prism="clr-namespace:Prism.Mvvm;assembly=Prism.Forms"
    prism:ViewModelLocator.AutowireViewModel="True"
	xmlns:local="clr-namespace:Simple.Views;assembly=Simple"
    x:Class="Simple.Views.MasterDetail"
    Title="Master Detail">


  <MasterDetailPage.Master>
    <ContentPage Title="Master">
	  <StackLayout>
	            <Button Text="M D Child" Command="{Binding Path=NavigateCommand}" CommandParameter="NavigationPage/MDChild" />
          <Button Text="M D Child1" Command="{Binding Path=NavigateCommand}" CommandParameter="NavigationPage/MDChild1" />
          <Button Text="M D Child2" Command="{Binding Path=NavigateCommand}" CommandParameter="NavigationPage/MDChild2" />
	  </StackLayout>
    </ContentPage>
  </MasterDetailPage.Master>



</MasterDetailPage>

